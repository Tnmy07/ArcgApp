{
  "expo": {
    "name": "arch",
    "owner": "tnmy07",
    "slug": "arch",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/archlogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    
    "splash": {
      "image": "./assets/images/archlogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/archlogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.arch_design"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/archlogo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "arch"
      }
    }
  }
}
