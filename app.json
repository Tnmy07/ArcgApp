{
  "expo": {
    "name": "arch",
    "owner": "archmain",
    "slug": "archapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/archlogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "cli": {
      "appVersionSource": "remote"
    },
    "splash": {
      "image": "./assets/images/archlogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/archlogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.arch_design"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "4fe0c5c7-7431-4205-8d04-1de8bc5a44d3"
      }
    }
  }
}
